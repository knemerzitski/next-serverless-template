{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install && graphql-codegen",
    "dynamodb:start": "cd ./mocks/dynamodb && docker compose up -d",
    "mongodb:start": "cd ./mocks/mongodb && docker compose up -d",
    "db:start": "npm run dynamodb:start & npm run mongodb:start",
    "db:stop": "(cd ./mocks/dynamodb && docker compose down) & (cd ./mocks/mongodb && docker compose down)",
    "api:dev": "cd ./mocks/graphql-lambda && npm run dev",
    "api:build": "cd ./mocks/graphql-lambda && npm run build",
    "api:start": "cd ./mocks/graphql-lambda && npm run start",
    "app:build": "next build",
    "app:dev": "next dev",
    "app:start": "http-server -p 3000 ./out",
    "start": "npm run api:start & npm run app:start",
    "graphql:generate": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",
    "test:jest:watch": "jest --watchAll",
    "cypress": "cypress open",
    "lint": "eslint . --cache",
    "test:jest": "jest",
    "test:component": "cypress run --component",
    "test:e2e:start-test": "start-test start http://127.0.0.1:3000 \"npm run test:e2e\"",
    "test:e2e": "cypress run --e2e",
    "deploy": "cdk deploy",
    "semantic-release": "SKIP_BY_SEMANTIC_RELEASE=true semantic-release"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.5",
    "@apollo/server": "^4.9.4",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.97.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.97.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.418.0",
    "@aws-sdk/client-dynamodb": "^3.418.0",
    "@aws-sdk/client-sts": "^3.428.0",
    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@faker-js/faker": "^8.1.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/aws-lambda": "^8.10.122",
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "autoprefixer": "^10.4.16",
    "aws-cdk-lib": "^2.97.0",
    "aws-lambda": "^1.0.7",
    "awscdk-resources-mongodbatlas": "^1.2.0",
    "body-parser": "^1.20.2",
    "bson": "^6.1.0",
    "classnames": "^2.3.2",
    "constructs": "^10.3.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cors": "^2.8.5",
    "cypress": "^13.3.1",
