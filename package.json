{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install && graphql-codegen",
    "dev": "next dev & npm run graphql:dev",
    "build": "next build",
    "start": "http-server -p 3000 ./out & npm run graphql:dev",
    "lint": "eslint . --cache",
    "test:unit:prepare": "docker compose -p unit -f docker-compose.unit.yml up -d",
    "test:unit": "jest",
    "test:unit:cleanup": "docker compose -p unit down",
    "test:unit:watch": "jest --watch",
    "test:e2e:prepare": "docker compose -p e2e -f docker-compose.e2e.yml up -d --wait",
    "test:e2e:run": "start-test start http://localhost:3000 \"cypress run --e2e\"",
    "test:e2e:cleanup": "docker compose -p e2e down",
    "test:e2e": "npm run test:e2e:prepare && (npm run test:e2e:run; npm run test:e2e:cleanup)",
    "test:component": "cypress run --component",
    "cypress": "cypress open",
    "graphql:dev": "nodemon --project=graphql-server/tsconfig.json graphql-server/index.ts",
    "graphql:generate": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",
    "semantic-release": "SKIP_BY_SEMANTIC_RELEASE=true semantic-release"
  },
  "validate-branch-name": {
    "pattern": "^(dev|test|main|release)$|^(feat|fix|hotfix|wip|merge)/(no-ref|issue-\\d+)/.+$"
  },
  "dependencies": {
    "@apollo/client": "^3.8.3",
    "@apollo/server": "^4.9.3",
    "classnames": "^2.3.2",
    "encoding": "^0.1.13",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.0",
    "mongoose": "^7.5.1",
    "next": "^13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@as-integrations/aws-lambda": "^3.1.0",
    "@as-integrations/next": "^2.0.2",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.97.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.97.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.418.0",
    "@aws-sdk/client-dynamodb": "^3.418.0",
    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/aws-lambda": "^8.10.122",
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.4",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.4",
    "autoprefixer": "10.4.15",
    "aws-cdk-lib": "^2.97.0",
    "body-parser": "^1.20.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cors": "^2.8.5",
    "cypress": "^13.1.0",
    "cypress-mongodb": "^6.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "express": "^4.18.2",
    "git-cz": "^4.9.0",
    "graphql-subscriptions": "^2.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "postcss": "8.4.30",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2",
    "uuid-mongodb": "^2.5.4",
    "validate-branch-name": "^1.3.0"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0"
  }
}
