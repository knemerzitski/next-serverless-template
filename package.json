{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install && graphql-codegen && (cd ./mocks/graphql-lambda && npm install)",
    "mock:server:dev": "cd ./mocks/graphql-server && npm run dev",
    "mock:lambda:dev": "cd ./mocks/graphql-lambda && npm run dev",
    "mock:lambda:build": "cd ./mocks/graphql-lambda && npm run build",
    "mock:lambda:start": "cd ./mocks/graphql-lambda && npm run start",
    "mock:dynamodb": "cd ./mocks/dynamodb && docker compose up -d",
    "mock:mongodb": "cd ./mocks/mongodb && docker compose up -d",
    "dev": "next dev",
    "lint": "eslint . --cache",
    "build": "next build",
    "start": "http-server -p 3000 ./out & (cd ./mocks/graphql-lambda && npm run build && npm run start)",
    "graphql:generate": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",
    "test:prepare": "(cd ./mocks/dynamodb && docker compose up -d --wait) && (cd ./mocks/mongodb && docker compose up -d)",
    "test:unit": "jest",
    "test:component": "cypress run --component",
    "test:e2e": "start-test start http://localhost:3000 \"cypress run --e2e\"",
    "test:cleanup": "(cd ./mocks/dynamodb && docker compose down) && (cd ./mocks/mongodb && docker compose down)",
    "cypress": "cypress open",
    "test:watch": "jest --watch",
    "semantic-release": "SKIP_BY_SEMANTIC_RELEASE=true semantic-release"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.3",
    "@apollo/server": "^4.9.3",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@as-integrations/next": "^2.0.2",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.97.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.97.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.418.0",
    "@aws-sdk/client-dynamodb": "^3.418.0",
    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/aws-lambda": "^8.10.122",
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "autoprefixer": "^10.4.15",
    "aws-cdk-lib": "^2.97.0",
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cors": "^2.8.5",
    "cypress": "^13.1.0",
    "cypress-mongodb": "^6.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "express": "^4.18.2",
    "git-cz": "^4.9.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^14.0.1",
    "mongoose": "^7.5.1",
    "next": "^13.4.12",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^22.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "uuid-mongodb": "^2.5.4",
    "validate-branch-name": "^1.3.0"
  },
  "validate-branch-name": {
    "pattern": "^(dev|test|main|release)$|^(feat|fix|hotfix|wip|merge)/.+$"
  }
}
